# Snyk API & Web Configuration
# Copy this file to .env and fill in your actual values

# Your Snyk API & Web (formerly Probely) API token
PROBELY_API_TOKEN=your_api_token_here

# API base URL (usually https://api.probely.com)
PROBELY_API_BASE_URL=https://api.probely.com

# Request timeout in seconds
REQUEST_TIMEOUT=30

# Maximum retry attempts
MAX_RETRIES=3
